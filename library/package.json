{
  "name": "@yanikkendler/web-utils",
  "version": "1.2.7",
  "description": "a collection of utility functions for web development in js/ts",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json & npm run docs",
    "docs": "(echo # API & echo. & echo ^<a name=\"root\"^>^</a^> & echo. & npx jsdoc2md \"dist/**/*.js\") > API.md"
  },
  "files": [
    "dist"
  ],
  "include": [
    "src/**/*.ts"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elYanuki/web-utils.git"
  },
  "author": "yanik kendler",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/elYanuki/web-utils/issues"
  },
  "homepage": "https://github.com/elYanuki/web-utils#readme",
  "devDependencies": {
    "jsdoc": "^4.0.5",
    "jsdoc-to-markdown": "^9.1.3",
    "typescript": "^5.9.3"
  }
}
